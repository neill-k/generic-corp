# Database
DATABASE_URL="postgresql://genericcorp:genericcorp@localhost:5432/genericcorp"

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379

# Agent execution (CLI runtime)
# Agents run via a configurable CLI runner (see apps/server/src/workers/cli/).
# Default behavior is "echo" mode (returns the prompt).
# To call a real CLI tool, override these:
# GENERIC_CORP_AGENT_CLI_COMMAND=bash
# GENERIC_CORP_AGENT_CLI_ARGS=-lc
# GENERIC_CORP_AGENT_CLI_SCRIPT='gemini -p "$AGENT_PROMPT"'

# Server
PORT=3000
NODE_ENV=development

# Client
VITE_WS_URL=http://localhost:3000
VITE_API_URL=http://localhost:3000/api
